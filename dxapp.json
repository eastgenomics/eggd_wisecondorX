{
  "name": "eggd_wisecondorX",
  "title": "eggd_wisecondorX",
  "summary": "App to run WisecondorX",
  "dxapi": "1.0.0",
  "version": "v1.0.0",
  "inputSpec": [
    {
      "name": "bam",
      "label": "Bams to process",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "binsize",
      "label": "Binsize (default=5e3)",
      "class": "int",
      "optional": true,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "something",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "interpreter": "bash",
    "release": "20.04",
    "distribution": "Ubuntu",
    "file": "src/eggd_wisecondorX.sh",
    "version": "0"
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x4"
        }
      }
    }
  }
}
